{
  "name": "dropbox-demo",
  "scheme": "com.bacon.dropbox",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "cover"
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": ["**/*"],
  "ios": {
    "supportsTablet": true
  }
}
